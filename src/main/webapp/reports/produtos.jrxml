	<parameter name="PRODUTO_DESCRICAO" class="java.lang.String"/>
	<parameter name="FABRICANTE_DESCRICAO" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT farmacia.produto.descricao AS pdescricao,
	farmacia.produto.preco,
	farmacia.produto.quantidade,
	farmacia.fabricante.descricao AS fdescricao
FROM farmacia.produto
	INNER JOIN farmacia.fabricante ON 
	 farmacia.produto.fabricante_codigo = farmacia.fabricante.codigo 
WHERE 
	 farmacia.produto.descricao LIKE $P{PRODUTO_DESCRICAO} 
	 AND farmacia.fabricante.descricao LIKE $P{FABRICANTE_DESCRICAO} 
ORDER BY farmacia.produto.descricao ASC,
 farmacia.fabricante.descricao ASC]]>
	</queryString>
